        <ns4:input id="pkiss_input_rna_sequences">
            <ns4:name xml:lang="en">RNA sequence(s)</ns4:name>
            <ns4:shortDescription xml:lang="en">RNA primary sequence(s)</ns4:shortDescription>
            <ns4:description xml:lang="en">
                
    <ns3:p>
A (multiple) FASTA file, containing RNA primary sequences.</ns3:p>
            </ns4:description>
            <ns4:type>Fasta_DNA</ns4:type>
            <ns4:handling>stdin</ns4:handling>
