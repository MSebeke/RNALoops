            </ns4:values>
            <ns4:values defaultValue="false">
                <ns4:key>B</ns4:key>
                <ns4:value>B</ns4:value>
                <ns4:name xml:lang="en">pKiss B: buying thoroughness by memory</ns4:name>
            </ns4:values>
            <ns4:values defaultValue="false">
                <ns4:key>C</ns4:key>
                <ns4:value>C</ns4:value>
                <ns4:name xml:lang="en">pKiss C: slow, low memory, but thorough</ns4:name>
            </ns4:values>
            <ns4:values defaultValue="false">
                <ns4:key>D</ns4:key>
                <ns4:value>D</ns4:value>
                <ns4:name xml:lang="en">pKiss D: very slow, but thorough</ns4:name>
            </ns4:values>
            <ns4:values defaultValue="false">
                <ns4:key>P</ns4:key>
                <ns4:value>P</ns4:value>
